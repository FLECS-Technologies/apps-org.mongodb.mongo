{
    "$schema": "https://raw.githubusercontent.com/FLECS-Technologies/app-manifest/2.x/manifest.schema.json",
    "_schemaVersion": "2.0.0",
    "app": "org.mongodb.mongo",
    "version": "##VERSION##",
    "revision": "0",
    "image": "flecs.azurecr.io/org.mongodb.mongo",
    "multiInstance": true,
    "env": [
        "MONGO_INITDB_ROOT_USERNAME=root",
        "MONGO_INITDB_ROOT_PASSWORD=example"
    ],
    "volumes": [
        "db:/data/db"
    ]
}
